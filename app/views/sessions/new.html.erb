<h3>Sign In</h3>

<form action="<%= session_url %>" method="post">

  <%= auth_token %>

  <label for="users-username">Username: </label>
  <input type="text"
         name="user[username]"
         value="<%= @user.username %>"
         id="users-username">
  <br>

  <label for="users-password">Password: </label>
  <input type="password"
         name="user[password]"
         id="users-password">
 <br>

 <input type="submit" value="Log In">

</form>
